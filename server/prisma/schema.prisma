
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Banner {
  id        String     @default(cuid()) @id
  title     String
  subTitle  String
  imageUrl  String
  reviews  Int
}

model Products {
  id        String     @default(cuid()) @id
  title     String
  imageUrl  String
  popularity  String
  for       String
  tags    String
  newArrival Int
  totalSale Int
  price Int
}