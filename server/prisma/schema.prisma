
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Banner {
  id  String     @default(cuid()) @id
  title String
  subTitle  String
  imageUrl  String
  reviews Int
}

model Male {
  id  String     @default(cuid()) @id
  name  String
  imageUrl  String
  popularity  String
  tags  String
  newArrival Int
  price Int
  discount Int
  stock Int
  comingSoon Int
  reviews Int
}

model Female {
  id  String     @default(cuid()) @id
  name  String
  imageUrl  String
  popularity  String
  tags  String
  newArrival Int
  price Int
  discount Int
  stock Int
  comingSoon Int
}

model MaleShoes {
  id  String     @default(cuid()) @id
  name  String
  imageUrl  String
  popularity  String
  tags  String
  newArrival Int
  price Int
  discount Int
  stock Int
  comingSoon Int
}

model FemaleShoes {
  id  String     @default(cuid()) @id
  name  String
  imageUrl  String
  popularity  String
  tags    String
  newArrival Int
  price Int
  discount Int
  stock Int
  comingSoon Int
}